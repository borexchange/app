#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/xampp/htdocs/dashboard/node_modules/.pnpm/nuxt@3.7.3_@parcel+watcher@2.3.0_@types+node@20.6.3_eslint@8.49.0_typescript@5.2.2_vue-tsc@1.8.11/node_modules/nuxt/bin/node_modules:/mnt/c/xampp/htdocs/dashboard/node_modules/.pnpm/nuxt@3.7.3_@parcel+watcher@2.3.0_@types+node@20.6.3_eslint@8.49.0_typescript@5.2.2_vue-tsc@1.8.11/node_modules/nuxt/node_modules:/mnt/c/xampp/htdocs/dashboard/node_modules/.pnpm/nuxt@3.7.3_@parcel+watcher@2.3.0_@types+node@20.6.3_eslint@8.49.0_typescript@5.2.2_vue-tsc@1.8.11/node_modules:/mnt/c/xampp/htdocs/dashboard/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/xampp/htdocs/dashboard/node_modules/.pnpm/nuxt@3.7.3_@parcel+watcher@2.3.0_@types+node@20.6.3_eslint@8.49.0_typescript@5.2.2_vue-tsc@1.8.11/node_modules/nuxt/bin/node_modules:/mnt/c/xampp/htdocs/dashboard/node_modules/.pnpm/nuxt@3.7.3_@parcel+watcher@2.3.0_@types+node@20.6.3_eslint@8.49.0_typescript@5.2.2_vue-tsc@1.8.11/node_modules/nuxt/node_modules:/mnt/c/xampp/htdocs/dashboard/node_modules/.pnpm/nuxt@3.7.3_@parcel+watcher@2.3.0_@types+node@20.6.3_eslint@8.49.0_typescript@5.2.2_vue-tsc@1.8.11/node_modules:/mnt/c/xampp/htdocs/dashboard/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/nuxt.mjs" "$@"
else
  exec node  "$basedir/../../bin/nuxt.mjs" "$@"
fi
